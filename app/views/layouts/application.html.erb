<!DOCTYPE html>
<html>
  <head>
    <title>React5X</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= yield :head %>
  </head>

  <body>
    <div class="body-container">
      <%= yield %>
    </div>
    <%= yield :scripts %>
  </body>
</html>
